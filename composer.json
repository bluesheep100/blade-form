{
    "version": "0.0.1",
    "name": "bluesheep100/blade-form",
    "description": "A group of easy-to-use Blade components for making forms with less code by adhering to conventions.",
    "keywords": [
        "Anders Aagaard Schütt",
        "laravel",
        "blade-form"
    ],
    "homepage": "https://github.com/bluesheep100/blade-form",
    "license": "MIT",
    "authors": [
        {
            "name": "Anders Aagaard Schütt",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0||^11.0",
        "illuminate/support": "^11.3",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.15",
        "nunomaduro/collision": "^8.1.1||^7.10.0"
    },
    "autoload": {
        "psr-4": {
            "Bluesheep\\BladeForm\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bluesheep\\BladeForm\\BladeFormServiceProvider"
            ],
            "aliases": {
                "BladeForm": "Bluesheep\\BladeForm\\Facades\\BladeForm"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
